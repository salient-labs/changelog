{
    "output": "build/dist/changelog.phar",
    "chmod": "0777",
    "files": [
        "bin/changelog"
    ],
    "finder": [
        {
            "in": "src"
        },
        {
            "in": "vendor",
            "notName": [
                "/^(?:LICENSE|README|composer\\.(?:json|lock))$|\\.md$/"
            ],
            "notPath": [
                "/^bin\\//",
                "/^(?:[^\\/]+\\/){2}(?:tests?(?:_|\\b).*|docs?)\\//i",
                "/^(?:[^\\/]+\\/){2}(?:phpunit|phpstan)\\b[^\\/]+$/i",
                "/^lkrms\\/util\\/(?:bin|lk-util|src\\/(?:Db|Sync))\\//",
                "/^lkrms\\/dice\\/src\\/Loader\\//",
                "/^psr\\/log\\/Psr\\/Log\\/Test\\//"
            ]
        }
    ],
    "alias": "changelog.phar",
    "banner": [
        "This file is part of salient/changelog",
        "",
        "(c) Luke Arms <luke@linacreative.com>",
        "",
        "For the full copyright and license information, please view the LICENSE",
        "file that was distributed with this source code."
    ],
    "compactors": [
        "KevinGH\\Box\\Compactor\\Json",
        "KevinGH\\Box\\Compactor\\Php"
    ]
}
